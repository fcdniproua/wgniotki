:[\\\"src\\\",\\\"poster\\\"],\\\"source\\\":\\\"src\\\",\\\"img\\\":\\\"src\\\",\\\"image\\\":\\\"xlink:href\\\"},\\\"buble\\\":{\\\"transforms\\\":{}}}!../../../node_modules/vue-loader/lib/selector?type=template&index=0!./ContactInfo.vue\"\n/* template functional */\nvar __vue_template_functional__ = false\n/* styles */\nvar __vue_styles__ = injectStyle\n/* scopeId */\nvar __vue_scopeId__ = \"data-v-13bfe662\"\n/* moduleIdentifier (server only) */\nvar __vue_module_identifier__ = null\nvar Component = normalizeComponent(\n  __vue_script__,\n  __vue_template__,\n  __vue_template_functional__,\n  __vue_styles__,\n  __vue_scopeId__,\n  __vue_module_identifier__\n)\n\nexport default Component.exports\n\n\n\n//////////////////\n// WEBPACK FOOTER\n// ./src/components/ContactInfo/ContactInfo.vue\n// module id = null\n// module chunks = ","<template>\n  <div class=\"checkout\" :class=\"{'is-loading': loading.trip}\">\n    <section class=\"octobus__section octobus__header\"  v-if=\"!loading.trip\">\n      <div :class=\"{ 'octobus__container': !isFullWidth }\">\n        <h1 class=\"octobus__subtitle is-size-4-touch is-size-3-desktop\">{{$t('checkout.title')}}</h1>\n      </div>\n    </section>\n    <section class=\"octobus__section route-info\"  v-if=\"!loading.trip\">\n      <div\n        :class=\"{'multi-route' : index !== 0, 'octobus__container': !isFullWidth}\"\n        v-for=\"(trip, index) in trip.info\"\n        :key=\"trip.raceId\"\n      >\n        <TripInfo\n          :trip=\"trip\"\n          :passcount=\"passcount\"\n        />\n      </div>\n    </section>\n    <section class=\"octobus__section checkout-trip\" v-if=\"!loading.trip\">\n      <div :class=\"{ 'octobus__container': !isFullWidth }\">\n        <Passenger\n          v-for=\"item in passcount\"\n          :key=\"item\"\n          :passenger=\"item\"\n          :trips=\"trip.info\"\n          :selectedPlace=\"placeByPassengers\"\n          @onUpdatePrice=\"updatePrice\"\n          @onUpdatePlace=\"updatePlace\"\n          @onUpdatePersonalInfo=\"updatePassenger\"\n        />\n      </div>\n    </section>\n    <section class=\"octobus__section contact-info-wrapper\"  v-if=\"!loading.trip\">\n      <div :class=\"{ 'octobus__container': !isFullWidth }\">\n        <ContactInfo\n          :email=\"email\"\n          :phone=\"phone\"\n          :tripId=\"trip.id\"\n          :paysys=\"paysys\"\n          @changeEmail=\"setEmail\"\n          @changePhone=\"setPhone\"\n          @paysys:changed=\"setPaysys\"\n        />\n\n        <div class=\"octobus__columns\" v-if=\"$frameparams.brand === 'OrionBus'\">\n          <div class=\"octobus__column\">\n            <p style=\"color: #c6004b;\">{{ $t('checkout.orion-msg') }}</p>\n          </div>\n        </div>\n\n        <div class=\"octobus__columns\">\n          <div class=\"octobus__column\">\n            <div class=\"octobus__field\">\n              <input\n                v-validate=\"'required'\"\n                class=\"is-checkradio\"\n                :class=\"{\n                  'is-danger': isError('term'),\n                  'is-primary': isSuccess('term'),\n                }\"\n                id=\"term\"\n                type=\"checkbox\"\n                name=\"term\"\n              >\n              <label for=\"term\" v-html=\"$t('checkout.term', { ofertaurl })\"></label>\n            </div>\n          </div>\n        </div>\n        <div class=\"octobus__columns is-vcentered\">\n          <div class=\"octobus__column\">\n            <div class=\"octobus__columns is-mobile\">\n              <div class=\"octobus__column is-flex-none\">\n                <p class=\"total is-size-3 is-inline-block-tablet\">{{$t('checkout.sum')}}</p>\n              </div>\n              <div class=\"octobus__column\">\n                <p class=\"is-inline-block-tablet total has-text-right-touch has-text-primary is-size-3 has-text-weight-semibold-touch\">{{totalPrice | currency(currencySymbol)}}</p>\n              </div>\n            </div>\n          </div>\n          <div :class=\"{'octobus__column': true, 'is-one-third-desktop': !canResv || !canResv}\">\n            <div class=\"octobus__columns is-mobile\">\n              <div class=\"octobus__column\" v-if=\"canSale\">\n                <a\n                  class=\"octobus__button is-primary is-fullwidth\"\n                  @click=\"onClickBuy()\"\n                  :disabled=\"!isSuccess('term')\"\n                  :class=\"{'is-loading': buying}\"\n                >{{$t('checkout.pay')}}</a>\n              </div>\n              <div class=\"octobus__column\" v-if=\"canResv\" :class=\"{ 'is-half is-offset-half': !canSale}\">\n                <a\n                  class=\"octobus__button is-primary is-fullwidth\"\n                  @click=\"onClickBuy(true)\"\n                  :disabled=\"!isSuccess('term')\"\n                  :class=\"{'is-loading': reserving}\"\n                >{{$t('checkout.reserv')}}</a>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n    <ErrorModal v-if=\"buyError\" :show=\"true\" :error=\"buyError\" @hideModal=\"onHideError\" />\n  </div>\n</template>\n\n<script>\n/* eslint object-shorthand: [\"error\", \"always\", { \"ignoreConstructors\": true }] */\n/* eslint-env es6 */\nimport api from '@/api';\nimport { getCountryCode, setOrderData, getBuyerId, isUrlLang } from '@/utils/utils';\nimport ErrorModal from '@/components/_common/ErrorModal';\nimport TripInfo from './TripInfo/TripInfo';\nimport Passenger from './Passenger/Passenger';\nimport ContactInfo from './ContactInfo/ContactInfo';\n\nexport default {\n  name: 'Checkout',\n  components: {\n    TripInfo,\n    Passenger,\n    ContactInfo,\n    ErrorModal,\n  },\n  data() {\n    return {\n      trip: {\n        info: [],\n        id: '',\n      },\n      locale: this.$framelocale,\n      priceByPassengers: {},\n      placeByPassengers: {},\n      passengers: {},\n      query: this.$route.query,\n      passcount: null,\n      email: '',\n      paysys: this.$frameparams.paysys[0],\n      phone: {\n        country: '+38',\n        number: '',\n      },\n      loading: {\n        trip: true,\n      },\n      buying: false,\n      reserving: false,\n      buyError: null,\n    };\n  },\n  watch: {\n    'trip.info'() {\n      if (this.trip.info.some(ticket => ticket.countryFrom === 'RU') && this.trip.info.some(ticket => ticket.countryTo === 'RU')) {\n        this.phone.country = '+7';\n      }\n    },\n  },\n  methods: {\n    onClickBuy(reserve = false) {\n      this.$validator.validateAll().then((validate) => {\n        if (validate === true) {\n          this.setProgress(reserve, true);\n          api.Buy.buyinit().then((resinit) => {\n            const { buyid, error: initError } = resinit;\n\n            if (initError) {\n              return Promise.reject();\n            }\n\n            const reserveMode = Number(reserve) === 1 ? this.$frameparams.reservemode : 0;\n            const buyParams = {\n              id: this.trip.id,\n              buyid,\n              reserveMode,\n              places: this.getPlacesData(),\n            };\n\n            if (reserveMode === 0) {\n              const returnToLocation = {\n                name: 'Success',\n                params: isUrlLang ? { locale: this.$UrlLang } : {},\n              };\n              const returnUrl = `${location.protocol}//${location.host}${this.$router.resolve(returnToLocation).route.path}`;\n              buyParams.paymentSystem = this.paysys;\n              buyParams.returnUrl = returnUrl;\n            }\n\n            const buyerId = getBuyerId();\n            if (buyerId) {\n              buyParams.buyerid = buyerId;\n            }\n\n            return api.Buy.buysite(buyParams).then((res) => {\n              this.setProgress(reserve, false);\n              /* eslint-disable no-param-reassign */\n              const { payref, error } = res;\n\n              if (error) {\n                if (buyerId && typeof error === 'object' && error.code === 'E_ACCESSDENY') {\n                  // just ignore the error expecting that sign in form will be shown to user\n                  return false;\n                }\n                return Promise.reject(error.name);\n              }\n\n              res.buyid = buyid;\n              res.reserve = reserve;\n              if (reserve) {\n                res.reserving = this.$frameparams.reserving;\n              }\n\n              setOrderData(res);\n              if (reserve) {\n                this.$router.push({ name: 'Success' });\n              } else {\n                location.href = payref;\n              }\n              return true;\n            });\n          }).catch((error) => {\n            this.setProgress(reserve, false);\n            this.buyError = (typeof error === 'string' || error instanceof String ? error : null) || this.$t('checkout.error');\n          });\n        }\n      });\n    },\n    onHideError() {\n      this.buyError = null;\n    },\n    setProgress(reserve, type) {\n      if (reserve) {\n        this.reserving = type;\n      } else {\n        this.buying = type;\n      }\n\n      return true;\n    },\n    isError(field) {\n      return this.errors.has(field);\n    },\n    isSuccess(field) {\n      return (\n        !this.errors.has(field) &&\n        this.fields[field] &&\n        this.fields[field].dirty\n      );\n    },\n    getPassengerData(currentPassengerPlaces, passengerId) {\n      return Object.keys(currentPassengerPlaces)\n        .map((raceId) => {\n          const ticket = currentPassengerPlaces[raceId];\n          const lgot = ticket.lgot;\n          const email = this.email;\n          const phone = `${this.phone.country}${this.phone.number}`;\n          const place = ticket.place;\n          const baggage = ticket.baggage || 0;\n          const { lastName, name, passport, isDocNeed, descript, ...additionalFields } = this.passengers[passengerId];\n          let additionalData = {};\n          if (additionalFields && additionalFields.birthDay) {\n            const [day, month, year] = additionalFields.birthDay.split('-');\n\n            additionalData = {\n              birthDay: `${year}-${month}-${day}`,\n            };\n          }\n\n          const baseFields = {\n            raceId,\n            lgot,\n            email,\n            phone,\n            baggage,\n            place,\n            name,\n            lastName,\n            passport,\n            descript,\n          };\n\n          if (!isDocNeed) {\n            return baseFields;\n          }\n\n          return { ...baseFields, ...additionalFields, ...additionalData };\n        });\n    },\n    getPlacesData() {\n      return Object.keys(this.passengers)\n        .map((passengerId) => {\n          const currentPassengerPlaces = this.placeByPassengers[passengerId];\n          return this.getPassengerData(currentPassengerPlaces, passengerId);\n        })\n        .reduce((prev, curr) => prev.concat(curr), []);\n    },\n    setEmail(value) {\n      this.email = value;\n    },\n    setPhone(value, type) {\n      this.phone[type] = value;\n    },\n    setPaysys(paysys) {\n      this.paysys = paysys;\n    },\n    updatePrice(passenger, totalPrice) {\n      this.$set(this.priceByPassengers, passenger, totalPrice);\n    },\n    updatePlace(passenger, place) {\n      this.$set(this.placeByPassengers, passenger, place);\n    },\n    updatePassenger(passenger, data) {\n      this.$set(this.passengers, passenger, data);\n    },\n    prefillUser() {\n      const { country, phone } = getCountryCode((this.$userData || {}).phone || this.$EventBus.$userData.phone);\n\n      this.email = (this.$userData || {}).email || this.$EventBus.$userData.email || '';\n      this.phone = {\n        country,\n        number: phone,\n      };\n    },\n  },\n  computed: {\n    totalPrice() {\n      const ids = Object.keys(this.priceByPassengers);\n\n      return ids.reduce((sum, id) => sum + Number(this.priceByPassengers[id]), 0);\n    },\n    currencySymbol() {\n      const name = this.$root.octobusCurrency ? this.$root.octobusCurrency.name : '';\n      const currency = name || this.$root.octobusCurrencyObj;\n\n      return typeof currency === 'object' ? currency.name : currency;\n    },\n    canResv() {\n      return !this.trip.info\n        .filter(trip => trip.canResv === 0).length;\n    },\n    canSale() {\n      return !this.trip.info\n        .filter(trip => trip.canSale === 0).length;\n    },\n    ofertaurl() {\n      return this.$frameparams.oferta || '#';\n    },\n    isFullWidth() {\n      return this.$fullwidth;\n    },\n  },\n  created() {\n    const { id } = this.query;\n    this.passcount = Number(localStorage.getItem('__octobus__search-cnt')) || 1;\n\n    api.Trip.info(id, this.passcount).then((res) => {\n      const { error } = res;\n\n      this.loading.trip = false;\n      if (error) {\n        this.error = error;\n      } else {\n        this.trip.id = id;\n        this.trip.info = res;\n      }\n    });\n\n    if (this.$userData || this.$EventBus.$userData) {\n      this.prefillUser();\n    }\n\n    this.$EventBus.$on('auth__signin', this.prefillUser);\n\n    this.$nextTick(() => {\n      this.$el.scrollIntoView({ behavior: 'smooth' });\n    });\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#app, #auth {\n  .checkout {\n\n    .is-flex-none {\n      flex: none !important;\n    }\n\n    p {\n      margin: 0;\n    }\n\n    &.is-loading {\n      height: 100vh;\n    }\n\n    .checkout-trip {\n      padding: 3rem 0.5rem;\n    }\n\n    .octobus__header,\n    .route-info {\n      border-bottom: 1px solid #ececec;\n    }\n\n    .route-info {\n      // box-shadow: 0 2px 4px rgba(0,0,0,.15);\n      padding-top: 0;\n      padding-bottom: 0;\n\n      .multi-route {\n        border-top: 1px dotted #ececec;\n      }\n    }\n\n  }\n}\n\n/* Large Devices, Wide Screens */\n@media only screen and (min-width : 768px) {\n  #app {\n    .checkout {\n      .octobus__header {\n        padding: 1rem 0.5rem;\n      }\n    }\n  }\n}\n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/components/Checkout.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"checkout\",class:{'is-loading': _vm.loading.trip}},[(!_vm.loading.trip)?_c('section',{staticClass:\"octobus__section octobus__header\"},[_c('div',{class:{ 'octobus__container': !_vm.isFullWidth }},[_c('h1',{staticClass:\"octobus__subtitle is-size-4-touch is-size-3-desktop\"},[_vm._v(_vm._s(_vm.$t('checkout.title')))])])]):_vm._e(),_vm._v(\" \"),(!_vm.loading.trip)?_c('section',{staticClass:\"octobus__section route-info\"},_vm._l((_vm.trip.info),function(trip,index){return _c('div',{key:trip.raceId,class:{'multi-route' : index !== 0, 'octobus__container': !_vm.isFullWidth}},[_c('TripInfo',{attrs:{\"trip\":trip,\"passcount\":_vm.passcount}})],1)})):_vm._e(),_vm._v(\" \"),(!_vm.loading.trip)?_c('section',{staticClass:\"octobus__section checkout-trip\"},[_c('div',{class:{ 'octobus__container': !_vm.isFullWidth }},_vm._l((_vm.passcount),function(item){return _c('Passenger',{key:item,attrs:{\"passenger\":item,\"trips\":_vm.trip.info,\"selectedPlace\":_vm.placeByPassengers},on:{\"onUpdatePrice\":_vm.updatePrice,\"onUpdatePlace\":_vm.updatePlace,\"onUpdatePersonalInfo\":_vm.updatePassenger}})}))]):_vm._e(),_vm._v(\" \"),(!_vm.loading.trip)?_c('section',{staticClass:\"octobus__section contact-info-wrapper\"},[_c('div',{class:{ 'octobus__container': !_vm.isFullWidth }},[_c('ContactInfo',{attrs:{\"email\":_vm.email,\"phone\":_vm.phone,\"tripId\":_vm.trip.id,\"paysys\":_vm.paysys},on:{\"changeEmail\":_vm.setEmail,\"changePhone\":_vm.setPhone,\"paysys:changed\":_vm.setPaysys}}),_vm._v(\" \"),(_vm.$frameparams.brand === 'OrionBus')?_c('div',{staticClass:\"octobus__columns\"},[_c('div',{staticClass:\"octobus__column\"},[_c('p',{staticStyle:{\"color\":\"#c6004b\"}},[_vm._v(_vm._s(_vm.$t('checkout.orion-msg')))])]