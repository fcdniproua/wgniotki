,\n    setSorting(event) {\n      const { sort } = event.target.dataset;\n\n      if (sort === this.sorting.field) {\n        this.sorting.type = this.sorting.type === 'asc' ? 'desc' : 'asc';\n      } else {\n        this.sorting.field = sort;\n        this.sorting.type = 'desc';\n      }\n      this.tickets = orderBy(this.tickets, obj => parseFloat(obj[this.sorting.field]), this.sorting.type);\n    },\n    async getTickets() {\n      this.loading.trips = true;\n      const { id } = this.$userData || this.$EventBus.$userData;\n      const tickets = await api.Content.buyertickets(id);\n\n      for (let i = 0; i < tickets.length; i += 1) {\n        tickets[i].dtDepSec = Date.parse(tickets[i].dtDep);\n        tickets[i].dtArrSec = Date.parse(tickets[i].dtArr);\n        tickets[i].wayTimeSec = ((tickets[i].wayHour * 60) * 60) + (tickets[i].wayMin * 60);\n      }\n\n      this.tickets = orderBy(tickets, obj => parseFloat(obj[this.sorting.field]), this.sorting.type);\n      this.loading.trips = false;\n    },\n  },\n  computed: {\n    isFullWidth() {\n      return this.$fullwidth;\n    },\n  },\n  mounted() {\n    this.getTickets();\n    this.$EventBus.$on('refund:done', this.getTickets);\n  },\n  beforeDestroy() {\n    this.$EventBus.$off('refund:done', this.getTickets);\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n#app {\n  .tickets {\n    .trips-menu {\n      margin-bottom: 1rem;\n      margin-left: 1.25rem;\n\n      .octobus__column {\n        cursor: pointer;\n\n        .fa-sort-down {\n          vertical-align: top;\n        }\n\n        .fa-sort-up {\n          vertical-align: bottom;\n        }\n\n        .fa-sort-up,\n        .fa-sort-down {\n          color: #808080;\n\n          &.is-active{\n            color: $color-main;\n          }\n        }\n      }\n    }\n  }\n}\n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/components/Tickets.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('section',{sta