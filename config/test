      this.user = null;\n      this.$EventBus.$emit('auth__logout');\n      this.showMenu = false;\n    },\n    onProfile() {\n      this.$EventBus.$emit('auth__profile', this.user);\n      this.showMenu = false;\n    },\n    onTickets() {\n      this.$EventBus.$emit('auth__tickets', this.user);\n      this.showMenu = false;\n    },\n  },\n  computed: {\n    shortName() {\n      const { name, surname } = this.user;\n\n      return `${name[0]}${surname[0]}`;\n    },\n    fullName() {\n      const { name, surname } = this.user;\n\n      return `${name} ${surname}`;\n    },\n    isFullWidth() {\n      return this.$fullwidth;\n    },\n    authTitle() {\n      return this.$authTitle;\n    },\n    userIcon() {\n      return faUser;\n    },\n    ticketIcon() {\n      return faTicketAlt;\n    },\n    userEditIcon() {\n      return faUserEdit;\n    },\n    signOutIcon() {\n      return faSignOutAlt;\n    },\n    signInIcon() {\n      return faSignInAlt;\n    },\n    registerIcon() {\n      return faAddressCard;\n    },\n  },\n  mounted() {\n    this.user = this.$userData || this.$EventBus.$userData || null;\n    this.position = this.$position;\n\n    this.$EventBus.$on('auth__session_expired', () => {\n      this.onLogout();\n      this.onShow('signin');\n    });\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.octobus__avatar {\n  color: #fff;\n  margin: 10px;\n  background-color: #673ab7;\n  width: 40px;\n  height: 40px;\n  display: flex;\n  position: relative;\n  overflow: hidden;\n  font-size: 1.25rem;\n  flex-shrink: 0;\n  align-items: center;\n  user-select: none;\n  border-radius: 50%;\n  justify-content: center;\n  cursor: pointer;\n}\n\n.octobus__dropdown-item {\n  cursor: pointer;\n}\n\n// left menu\n\nlabel.in-flow {\n  position: relative;\n  left: auto;\n  top: auto;\n}\n\n.octobus__nav {\n  width: 320px;\n  min-width: 320px;\n  height: 100%;\n  position: fixed;\n  top: 0;\n  bottom: 0;\n  margin: 0;\n  left: -320px;\n  padding: 15px 20px;\n  -webkit-transi