 cancelled\n          next(vm => vm.setInitializationError(error));\n        } else {\n          next({ name: 'Error', replace: true, params: isUrlLang ? { locale: to.params.locale } : {} });\n        }\n      })\n      .catch((error) => {\n        // 5xx HTTP errors\n        next(vm => vm.setInitializationError(error));\n      });\n  },\n  data() {\n    return {\n      urlLang: this.$UrlLang,\n      locale: this.$framelocale,\n      tickets: {},\n      downloadLink: '',\n      loading: {\n        link: false,\n      },\n      dataFile: '',\n      downloadError: null,\n      initialized: false,\n      initializationError: null,\n    };\n  },\n  props: {\n  },\n  methods: {\n    setInitialized() {\n      this.initialized = true;\n    },\n    setInitializationError(error) {\n      this.initializationError = this.getErrorText(error);\n      this.setInitialized();\n    },\n    getErrorText(error) {\n      if (error instanceof Object && error.name) {\n        return error.name + (error.param ? ` (${error.param})` : '');\n      }\n      if (error instanceof Object && error.status) {\n        return `${error.status} ${error.statusText || ''}`;\n      }\n      return (typeof error === 'string' || error instanceof String ? error : null) || 'error';\n    },\n    downloadTicket() {\n      if (this.tickets.buyid) {\n        this.loading.link = true;\n\n        api.Content.exportPDF({ buyId: this.tickets.buyid, justBought: true }).then((response) => {\n          this.loading.link = false;\n          if (!response.ok) {\n            return errorFromBlob(response.body).then(err => Promise.reject(err));\n          }\n\n          FileSaver.saveAs(response.body, getFileName(response));\n          clearOrderFlowData();\n          return true;\n        }, () => {\n          this.loading.link = false;\n        }).catch((error) => {\n          this.downloadError = (typeof error === 'string' || error instanceof String ? error : null) || this.$t('success.downloadError');\n        });\n      }\n    },\n    onHideDownloadError() {\n      this.downloadError = null;\n    },\n    print() {\n      window.print();\n      clearOrderFlowData();\n    },\n  },\n  computed: {\n    downoladIcon() {\n      return faDownload;\n    },\n    printIcon() {\n      return faPrint;\n    },\n    isTicketsData() {\n      return Boolean(this.tickets.buyid);\n    },\n    brand() {\n      return this.$frameparams.brand;\n    },\n    isFullWidth() {\n      return this.$fullwidth;\n    },\n  },\n  filters: {\n  },\n  created() {\n    this.tickets = getOrderData() || {};\n  },\n};\n</script>\n\n<style lang=\"scss\">\n@media print {\n  .noprint {\n    display: none !important\n  }\n  .wrapper>div {\n    overflow: visible !important\n  }\n  .prepere-for-print {\n    display: block !important\n  }\n  .boarding-ticket .ticket-page-break {\n    page-break-after: always\n  }\n}\n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/components/Success.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return (_vm.initialized)?_c('div',[(_vm.initializationError)?_c('section',{staticClass:\"octobus__section error\"},[_c('div',{class:{ 'octobus__container': !_vm.isFullWidth }},[_c('div',{staticClass:\"octobus__columns content\"},[_c('div',{staticClass:\"octobus__column has-text-centered is-size-4\"},[_vm._v(\"\\n          \"+_vm._s(_vm.$t('success.initError'))),_c('br'),_vm._v(\" \"),_c('small',[_vm._v(\"(\"+_vm._s(_vm.initializationError)+\")\")])])])])]):_c('section',{staticClass:\"octobus__section success\"},[(_vm.isTicketsData)?_c('div',{class:{ 'octobus__container': !_vm.isFullWidth }},[_c('div',{staticClass:\"octobus__columns noprint has-text-centered\"},[(_vm.tickets.reserve)?_c('div',{staticClass:\"octobus__column\",domProps:{\"innerHTML\":_vm._s(_vm.$t('success.accepted'))}}):_c('div',{staticClass:\"octobus__column\"},[_c('h2',{staticClass:\"octobus__title is-3\"},[_vm._v(_vm._s(_vm.$t('success.thankyou', {brand: _vm.brand})))]),_vm._v(\" \"),_c(