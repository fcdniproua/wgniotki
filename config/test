} });\n      }\n    },\n    toggle(field) {\n      this[field] = !this[field];\n    },\n    async downloadTicket() {\n      const { buyId } = this.trip;\n\n      if (!buyId) { return true; }\n\n      this.loading.link = true;\n\n      try {\n        const response = await api.Content.exportPDF({ buyId });\n        const fileName = getFileName(response);\n\n        if (!response.ok) {\n          const err = await errorFromBlob(response.body);\n          throw new Error(err);\n        }\n\n        FileSaver.saveAs(response.body, fileName);\n      } finally {\n        this.loading.link = false;\n      }\n      return true;\n    },\n    printTicket() {\n      const { buyId: buyid } = this.trip;\n\n      this.$emit('printTicket', buyid);\n    },\n    cancelTicket() {\n      this.$emit('showRfund', {\n        buyId: this.trip.buyId,\n        ticketId: this.trip.ticketId,\n      });\n    },\n    showDetails() {\n      this.show = !this.show;\n\n      if (this.show && !this.route.forward.length) {\n        const { tripId } = this.trip;\n\n        if (tripId) {\n          api.Trip.route(tripId).then((res) => {\n            this.loading.route = false;\n            const { forward, backward } = res;\n            this.route.forward = forward || [];\n            this.route.backward = backward || [];\n          });\n        }\n      }\n    },\n  },\n  computed: {\n    arrowDownLong() {\n      return faLongArrowAltDown;\n    },\n    isBack() {\n      return Boolean(this.trip.backTripId);\n    },\n    downloadIcon() {\n      return faDownload;\n    },\n    printIcon() {\n      return faPrint;\n    },\n    cancelIcon() {\n      return faBan;\n    },\n    currencySymbol() {\n      const name = this.$root.octobusCurrency ? this.$root.octobusCurrency.name : '';\n      const currency = name || this.$root.octobusCurrencyObj;\n\n      return typeof currency === 'object' ? currency.name : currency;\n    },\n    iconArrow() {\n      return this.show || this.showBack ? faAngleUp : faAngleDown;\n    },\n    isFullWidth() {\n      return this.$fullwidth;\n    },\n    pathToCheckout() {\n      const { children } = this.$router.options.routes[1];\n      const route = children.find(r => r.name === 'Checkout');\n\n      return this.$search ? `/${route.path}` : route.path;\n    },\n  },\n  filters: {\n    time(dateSring) {\n      return moment(dateSring).format('HH:mm');\n    },\n    date(dateSring) {\n      return moment(dateSring).format('ddd, Do MMMM');\n    },\n    shortDate(dateSring) {\n      return moment(dateSring).format('ddd, Do MMM');\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n\n/* Small Devices, Tablets */\n@media only screen and (max-width : 1023px) {\n  #app, #auth {\n    .trip-info {\n\n      .time-block {\n        margin-right: 0.7rem !important;\n      }\n    }\n  }\n}\n\n#app, #auth {\n  .ticket {\n    .is-divider {\n      border-top-width: 0.01rem;\n\n      &:after {\n        padding: 0.3rem .8rem;\n      }\n    }\n\n    .details-button {\n      cursor: pointer;\n    }\n\n    .old-price {\n      text-decoration: line-through;\n    }\n\n    .octobus__cancel,\n    .octobus__print,\n    .octobus__download {\n      cursor: pointer;\n      display: inline-block;\n    }\n  }\n}\n\n/* Small Devices, Tablets */\n@media only screen and (max-width : 768px) {\n  #app, #auth {\n    .ticket {\n      line-height: 1;\n      padding: 0.5rem;\n\n      .address-block {\n        font-size: 16px;\n        font-weight: 300;\n        line-height: 17px;\n        max-width: calc(100% - 100px);\n      }\n\n      .city-departure,\n      .city-arrived {\n        overflow: hidden;\n        white-space: nowrap;\n        text-overflow: ellipsis;\n      }\n\n      .time-block {\n        min-width: 90px;\n      }\n\n      .time-departure,\n      .time-arrived {\n        font-size: 1.1rem;\n      }\n\n      .date-departure,\n      .date-arrived {\n        font-size: 12px;\n      }\n\n      .trip-arrow-div {\n        padding-left: 1rem;\n      }\n      .d