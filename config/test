 skype,\n      };\n      this.loading = true;\n\n      try {\n        const response = await api.Authentication.signup(profile);\n\n        if (response.error) {\n          this.errorSignup(response);\n        } else {\n          this.successSignup();\n          // this.onHideModal();\n        }\n      } catch (e) {\n        throw e;\n      }\n\n      this.loading = false;\n    },\n  },\n  computed: {\n    mask() {\n      const phone = this.countryCode;\n      const mask = getMask(phone);\n\n      return mask;\n    },\n    userIcon() {\n      return faUser;\n    },\n    emailIcon() {\n      return faEnvelope;\n    },\n    passwordIcon() {\n      return faKey;\n    },\n    phoneNumber() {\n      return this.countryCode + this.phone.replace(/(\\)|\\(|-| )/g, '');\n    },\n  },\n};\n</script>\n\n\n\n// WEBPACK FOOTER //\n// src/components/_common/Signup.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"octobus__modal\",class:{ 'is-active' : _vm.show }},[_c('div',{staticClass:\"octobus__modal-background\"}),_vm._v(\" \"),_c('div',{staticClass:\"octobus__modal-card\"},[_c('header',{staticClass:\"octobus__modal-card-head\"},[_c('p',{staticClass:\"octobus__modal-card-title\"},[_vm._v(_vm._s(_vm.$t('auth.signup.title')))]),_vm._v(\" \"),_c('button',{staticClass:\"octobus__delete\",attrs:{\"aria-label\":\"close\"},on:{\"click\":_vm.onHideModal}})]),_vm._v(\" \"),_c('section',{staticClass:\"octobus__modal-card-body\"},[(_vm.error.name)?_c('p',{staticClass:\"octobus__signin-error has-text-danger has-text-centered\"},[_vm._v(_vm._s(_vm.error.name))]):_c('p',{staticClass:\"octobus__signin-error\"},[_vm._v(\" \")]),_vm._v(\" \"),_c('div',{staticClass:\"octobus__columns\"},[_c('div',{staticClass:\"octobus__column\"},[_c('div',{staticClass:\"octobus__field\"},[_c('label',{staticClass:\"octobus__label is-size-7-touch has-text-grey has-text-weight-light\",attrs:{\"for\":\"signup__firstname\"}},[_vm._v(\"\\n  