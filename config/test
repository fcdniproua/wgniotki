             {{seat.number}}\n                    </div>\n                  </div>\n                </div>\n              </template>\n              <template v-else>\n                <div class=\"floor\" v-if=\"busTemplate.matrix && busTemplate.matrix.length > 1\">\n                  <span\n                    v-for=\"(_, index) in busTemplate.matrix\"\n                    :class=\"{'floor--btn': true, active: floor === index + 1}\"\n                    :key=\"index\"\n                    @click=\"setFloor(index + 1)\"\n                  >{{ $t('bus.floor', { floor: index + 1 }) }}</span>\n                </div>\n                <div class=\"bus\">\n                  <div v-for=\"(row, index) in seatsTempl\" :key=\"index\" class=\"seats-row\">\n                    <div\n                      class=\"seat octobus__columns is-vcentered\"\n                      v-for=\"seat in row\"\n                      :key=\"seat.id\"\n                      :class=\"{\n                        'empty': seat.empty,\n                        'seat-free': seat.free,\n                        'selected': seat.selected,\n                        'seat-disabled': !seat.free,\n                        'second-side': seat.secondSide,\n                      }\"\n                      @click=\"setSeat(seat.number, seat.free, seat.selected)\"\n                    >\n                      <span v-if=\"!seat.empty\" class=\"seat--chair\">\n                        <svg-icon name='chair' width=\"40\" height=\"40\" :color=\"getColourForSeat(seat)\" />\n                      </span>\n                      <span class=\"seat--number\">{{seat.number}}</span>\n                      </div>\n                  </div>\n                  <div class=\"seats-row--first\" v-if=\"checkFirstPlace()\">\n                    <svg-icon name='wheel' width=\"40\" height=\"40\" color=\"r-#ccc\" />\n                  </div>\n                </div>\n              </template>\n            </div>\n          </div>\n        </