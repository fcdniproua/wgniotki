      @selectedLgotes=\"setLgotes\"\n          />\n        </div>\n        <div class=\"octobus__column is-hidden-mobile is-paddingless is-narrow-tablet is-2-tablet is-1-desktop has-text-right-tablet\">\n          <p>&nbsp;</p>\n          <Price :price=\"getTicketPrice(trip)\" />\n        </div>\n      </div>\n      <div class=\"octobus__columns\" :key=\"`${trip.raceId}-desc`\">\n        <div class=\"octobus__column is-paddingless\">\n          <div class=\"octobus__field\">\n            <label class=\"octobus__label is-size-7-touch has-text-grey has-text-weight-light\" :for=\"`passenger__desc_${id}`\">{{$t('passengerForm.description')}}</label>\n            <p class=\"octobus__control\">\n              <input\n                class=\"octobus__input is-medium\"\n                type=\"text\"\n                :placeholder=\"$t('passengerForm.description-placeholder')\"\n                :id=\"`passenger__desc_${id}`\"\n                :name=\"`passenger__desc_${id}`\"\n                v-model=\"descript\"\n              >\n            </p>\n          </div>\n        </div>\n      </div>\n      <hr v-if=\"trips.length !== (index + 1)\" :key=\"index\" />\n    </template>\n  </div>\n</template>\n\n<script>\nimport PassengerTitle from './PassengerTitle';\nimport PassengerForm from './PassengerForm';\nimport Bus from './Bus';\nimport Nobus from './Nobus';\nimport Discount from './Discount';\nimport Price from './Price';\nimport Baggage from './Baggage';\n\nexport default {\n  name: 'Passenger',\n  components: {\n    PassengerTitle,\n    PassengerForm,\n    Bus,\n    Nobus,\n    Discount,\n    Price,\n    Baggage,\n  },\n  inject: ['$validator'],\n  props: {\n    //   \n    passenger: {\n      type: Number,\n      default() {\n        return 1;\n      },\n    },\n    //  \n    trips: {\n      type: Array,\n      required: true,\n    },\n    //  \n    selectedPlace: {\n      type: Object,\n    },\n  },\n  data() {\n    return {\n      id: Math.random(),\n      name: '', //  \n      lastName: '', //  \n      passport: '', //     \n      descript: '', // \n      places: {},\n      birthDay: '', //   \n      citizen: '', //  \n      docType: '', //   \n      sex: '', //  \n    };\n  },\n  watch: {\n    descript() {\n      this.sendNewData();\n    },\n  },\n  methods: {\n    getRoute(departure, arrived) {\n      return `${departure} - ${arrived}`;\n    },\n    setBaggage(value, raceId) {\n      this.$set(this.places, raceId, { ...this.places[raceId], ...{ baggage: value } });\n      this.$emit('onUpdatePrice', this.id, this.totalPrice);\n      this.sendNewData();\n    },\n    setLgotes(value, raceId) {\n      this.$set(this.places, raceId, { ...this.places[raceId], ...{ lgot: value.code } });\n      this.$emit('onUpdatePrice', this.id, this.totalPrice);\n      this.sendNewData();\n    },\n    getLgot(raceId) {\n      return this.places[raceId] ? this.places[raceId].lgot : '';\n    },\n    setPromoCode(checkPromoCodeResponse) {\n      const data = checkPromoCodeResponse.find(tripResult => tripResult.applicable);\n      if (data) {\n        this.$set(this.places, data.raceId, { ...this.places[data.raceId], ...{ lgot: data.autoLgot.code, promoCode: data.autoLgot } });\n        this.$emit('onUpdatePrice', this.id, this.totalPrice);\n      }\n    },\n    getPromoCode(raceId) {\n      return this.places[raceId] ? this.places[raceId].promoCode : null;\n    },\n    setPlace(value, raceId) {\n      this.$set(this.places, raceId, { ...this.places[raceId], ...{ place: value.num } }); // set free place\n      this.$emit('onUpdatePlace', this.id, this.places);\n      this.sendNewData();\n    },\n    setPersonalInfo(data) {\n      this[data.field] = data.value;\n      this.sendNewData();\n    },\n    sendNewData() {\n      const { name, lastName, passport, birthDay, citizen, docType, sex, isDocNeed, descript } = this;\n\n      this.$emit('onUpdatePersonalInfo', this.id, {\n        name,\n        lastName,\n        passport,\n        birthDay,\n        descript,\n        citizen,\n        docType,\n        sex,\n        isDocNeed,\n      });\n    },\n    getTicketPrice(trip) {\n      const place = this.places[trip.raceId] || {};\n      const baseFarePrice = Number(trip.pricePass);\n\n      let farePrice = 0;\n      if (place.lgot) {\n        farePrice = [\n          ...(trip.lgotes || []),\n          ...(trip.autoLgot ? [trip.autoLgot] : []),\n          ...(place.promoCode ? [place.promoCode] : []),\n        ].find(lgot => lgot.code === place.lgot).price;\n      } else {\n        farePrice = baseFarePrice;\n      }\n\n      let baggageFee = 0;\n      if (place.baggage && +trip.priceBagg > 0) {\n        baggageFee = place.baggage * trip.priceBagg;\n      }\n\n      return {\n        total: Number(farePrice) + baggageFee,\n        withoutDiscount: Number(baseFarePrice) + baggageFee,\n      };\n    },\n    getFreePlaces(places, raceId, autoSelect) {\n      const selectedPlaces = this.selectedPlace;\n      const id = this.id;\n      let filteredPlaces = [];\n\n      if (autoSelect) {\n        // autoplace\n        filteredPlaces = places.filter((place) => {\n          const num = place.num;\n\n          const isSelected = Boolean(Object.keys(selectedPlaces)\n            .filter(passenger => selectedPlaces[passenger][raceId].place === num).length);\n\n          return !isSelected;\n        });\n      } else {\n        // bus\n        filteredPlaces = places.filter((place) => {\n          /* eslint-disable no-else-return */\n          const num = place.num;\n\n          const isSelected = Boolean(Object.keys(selectedPlaces).filter((passenger) => {\n            if (passenger !== String(id) && selectedPlaces[passenger][raceId]) {\n              return selectedPlaces[passenger][raceId].place === num;\n            } else {\n              return false;\n            }\n          }).length);\n\n          return !isSelected;\n        });\n      }\n\n      return filteredPlaces;\n    },\n  },\n  computed: {\n    isDocNeed() {\n      return this.trips.some(trip => trip.countryFrom === 'RU' && trip.simpleForm !== 1);\n    },\n    totalPrice() {\n      let total = 0;\n\n      if (this.trips) {\n        Object.keys(this.places).forEach((raceId) => {\n          const trip = this.trips.find(cTrip => cTrip.raceId === raceId);\n          total += this.getTicketPrice(trip).total;\n        });\n\n        return Number(total.toFixed(2));\n      }\n\n      return total;\n    },\n  },\n  mounted() {\n    this.$EventBus.$on('promocode__set', this.setPromoCode);\n  },\n  beforeDestroy() {\n    this.$EventBus.$off('promocode__set', this.setPromoCode);\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n#app, #auth {\n  .is-divider {\n    border-top-width: 0.01rem;\n\n    &:after {\n      padding: 0.3rem .8rem;\n    }\n  }\n\n  @media only screen and (max-width : 768px) {\n    .is-paddingless-mobile {\n      padding: 0;\n    }\n  }\n}\n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/components/Passenger/Passenger.vue","var normalizeComponent = require(\"!../../../node_modules/vue-loader/lib/component-normalizer\")\n/* script */\nexport * from \"!!babel-loader!../../../node_modules/vue-loader/lib/selector?type=script&index=0!./Baggage.vue\"\nimport __vue_script__ from \"!!babel-loader!../../../node_modules/vue-loader/lib/selector?type=script&index=0!./Baggage.vue\"\n/* template */\nimport __vue_template__ from \"!!../../../node_modules/vue-loader/lib/template-compiler/index?{\\\"id\\\":\\\"data-v-8ad6e6da\\\",\\\"hasScoped\\\":false,\\\"transformToRequire\\\":{\\\"video\\\":[\\\"src\\\",\\\"poster\\\"],\\\"source\\\":\\\"src\\\",\\\"img\\\":\\\"src\\\",\\\"image\\\":\\\"xlink:href\\\"},\\\"buble\\\":{\\\"transforms\\\":{}}}!../../../node_modules/vue-loader/lib/selector?type=template&index=0!./Baggage.vue\"\n/* template functional */\nvar __vue_template_functional__ = false\n/* styles */\nvar __vue_styles__ = null\n/* scopeId */\nvar __vue_scopeId__ = null\n/* moduleIdentifier (server only) */\nvar __vue_modul