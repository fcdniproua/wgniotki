-bus\" v-for=\"(route, rkey) in routes\" :key=\"route.raceNum\">\n      <div class=\"route-station\" v-for=\"(station, key) in route.route\" :key=\"station.stCode\">\n        <div class=\"octobus__columns is-vcentered\" v-if=\"getLabel(key, route.route.length, rkey, routes.length) === false\">\n          <div class=\"octobus__column is-one-third\">\n            <p class=\"station__change__label\">{{$t('ticket.routeChange')}}</p>\n          </div>\n          <div class=\"octobus__column is-two-thirds\">\n            <div class=\"octobus__columns is-vcentered\">\n              <div class=\"octobus__column is-narrow\">\n                <font-awesome-icon :icon=\"iconBus\" class=\"station-bus\" />\n                <font-awesome-icon :icon=\"iconArrow\" class=\"station-arrow\" />\n                <font-awesome-icon :icon=\"iconBus\" class=\"station-bus\" />\n              </div>\n              <div class=\"station__change octobus__column\">\n                <p class=\"station__change__time\">\n                  <span>{{$t('ticket.routeChangeTime')}}</span>\n                  <span class=\"has-text-weight-semibold is-size-6\">{{`${route.changehours}:${route.changeminutes}`}}</span>\n                </p>\n                <p class=\"station__change__name\">{{station.stArrName}}</p>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"octobus__columns\" v-if=\"!key\">\n          <div class=\"octobus__column is-offset-2-desktop\">{{$t('ticket.routeRaceName', {raceName: route.raceName})}}</div>\n        </div>\n        <div class=\"octobus__columns is-vcentered\">\n          <div class=\"octobus__column is-one-third\" :class=\"{'is-paddingless': !getLabel(key, route.route.length, rkey, routes.length)}\">\n            <Label\n              :label=\"getLabel(key, route.route.length, rkey, routes.length)\"\n            />\n          </div>\n          <div class=\"octobus__column is-two-thirds station-block\">\n            <p class=\"st