) => {\n  const buyerToken = getBuyerToken();\n  if (buyerToken) {\n    req.set('X-Buyer-Session', buyerToken);\n  }\n};\nconst handleBuyerSessionErrors = (res) => {\n  const buyerSessionExpired = res.status === 400 && res.body.error && res.body.error.code === 'E_ACCESSDENY';\n  if (buyerSessionExpired) {\n    getEventBus().$emit('auth__session_expired', res);\n  }\n  return res;\n};\n\nconst requests = {\n  get: ({ url, query = {}, withBuyerSession = false, withCurrency = false }) => {\n    let request = superagent\n      .get(`${API_ROOT}${url}`)\n      .query({ lang })\n      .query(query)\n      .use(tokenPlugin);\n    if (withCurrency) {\n      request = request.query({ currency: currency() });\n    }\n    if (withBuyerSession) {\n      request = request.use(buyerTokenPlugin);\n    }\n    return request\n      .responseType('json')\n      .ok(res => res.status < 500)\n      .then(res => (withBuyerSession ? handleBuyerSessionErrors(res) : res))\n      .then(extractResponseBody);\n  },\n  getBlob: ({ url, withBuyerSession = false }) => {\n    let request = superagent\n      .get(`${API_ROOT}${url}`)\n      .query({ lang })\n      .use(tokenPlugin);\n    if (withBuyerSession) {\n      request = request.use(buyerTokenPlugin);\n    }\n    return request\n      .responseType('blob')\n      .ok(res => res.status < 500)\n      .then(res => (withBuyerSession ? handleBuyerSessionErrors(res) : res));\n  },\n  post: ({ url, params, withBuyerSession = false }) => {\n    let request = superagent\n      .post(`${API_ROOT}${url}`)\n      .type('form')\n      .send(params)\n      .use(tokenPlugin);\n    if (withBuyerSession) {\n      request = request.use(buyerTokenPlugin);\n    }\n    return request\n      .ok(res => res.status < 500)\n      .then(res => (withBuyerSession ? handleBuyerSessionErrors(res) : res))\n      .then(extractResponseBody);\n  },\n};\n\nconst Content = {\n  cities: () =>\n    requests.get({ url: '/cities', query: { wcountry: 1 }, withCurrency: true }),\n 