n      this.isDirty = this.isChange();\n    },\n  },\n  methods: {\n    updatePhone(value) {\n      const formattedValue = value.trim().replace(/[^+\\d]/g, '');\n      this.phone = formattedValue;\n      return true;\n    },\n    isChange() {\n      return Object.keys(this.$validator.flags).some(item => this.$validator.flags[item].dirty);\n    },\n    async onSubmit() {\n      const validateSignin = await this.$validator.validateAll();\n\n      if (validateSignin === true && this.isDirty) {\n        this.sendSignup();\n        this.$validator.reset();\n      }\n    },\n    errorSignup(res) {\n      this.error = res.error;\n    },\n    successSignup() {\n      this.$toasted.show(this.$t('profile.success'));\n    },\n    async sendSignup() {\n      const { phoneNumber: phone, firstname: name, lastname: surname, password, email, viber, telegram, watsapp, skype, id } = this;\n      const profile = {\n        phone,\n        name,\n        surname,\n        email,\n        password,\n        viber,\n        telegram,\n        watsapp,\n        skype,\n      };\n      this.loading = true;\n\n      try {\n        const response = await api.Authentication.update(id, profile);\n\n        if (response.error) {\n          this.errorSignup(response);\n        } else {\n          this.successSignup();\n        }\n      } catch (e) {\n        throw e;\n      }\n\n      this.loading = false;\n    },\n  },\n};\n</script>\n\n\n\n// WEBPACK FOOTER //\n// src/components/Profile.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('section',{staticClass:\"octobus__section profile\"},[_c('div',{class:{ 'octobus__container': !_vm.isFullWidth }},[_c('div',{staticClass:\"octobus__columns content\"},[_c('div',{staticClass:\"octobus__column has-text-centered is-size-4\"},[_vm._v(_vm._s(_vm.$t('profile.title')))])]),_vm._v(\" \"),_c('div',{staticClass:\"octobus__columns content\"},[_c('div',{staticClass:\"octobus__column\"},[_c('div',{static