lock-touch time-block\"></div>\n        <div class=\"is-inline-block-touch\">\n          <span class=\"is-hidden-desktop\">{{$t('ticket.free')}}</span>\n          <span class=\"places\">{{ trip.backPlaces}}</span>\n        </div>\n      </div>\n      <div class=\"octobus__column\">\n        <div class=\"octobus__columns is-mobile has-text-weight-bold has-text-success is-vcentered\">\n          <div class=\"octobus__column\">\n            <p class=\"old-price has-text-dark has-text-weight-normal\" v-if=\"trip.backDiscPerc\">{{ trip.backPrice | currency(currencySymbol) }}</p>\n            <p class=\"ticket__price\">{{ trip.backDiscPerc ? trip.backDiscPrice : trip.backPrice | currency(currencySymbol) }}</p>\n          </div>\n          <div class=\"octobus__column is-hidden-desktop\">\n            <a\n              class=\"octobus__button is-primary is-fullwidth\"\n              @click=\"handleToCheckout\"\n            >{{$t('ticket.buy')}}</a>\n          </div>\n        </div>\n        <div class=\"octobus__columns\">\n          <div class=\"octobus__column\">\n            <span>{{$t('ticket.sum')}}</span> <span class=\"has-text-weight-bold has-text-success\">{{trip.fullPrice | currency(currencySymbol)}}</span>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"octobus__columns is-vcentered\" v-if=\"isBack\">\n      <div class=\"octobus__column is-three-fifths has-text-centered-touch is-size-7 has-text-weight-light\">\n        <p class=\"carrier\">{{$t('ticket.carrier', {carrier: trip.backCarrier})}}</p>\n      </div>\n      <div class=\"octobus__column has-text-centered-touch is-size-6 has-text-weight-bold\">\n        <p class=\"details-button\" @click.prevent=\"toggle('showBack')\">\n          {{$t('ticket.showMore')}}\n          <font-awesome-icon :icon=\"iconArrow\" />\n        </p>\n      </div>\n      <div class=\"octobus__column is-hidden-touch\">\n        <a\n          class=\"octobus__button is-primary\"\n          @click=\"handleTo