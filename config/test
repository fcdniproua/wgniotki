 }\n      if (seat.selected) { return 'r-#65BB69'; }\n\n      return 'r-#777';\n    },\n    toggleBus(off) {\n      this.showBus = (off === true) ? false : !this.showBus;\n    },\n    setSeat(value, isFree, isSelected) {\n      if (isFree && !isSelected) {\n        this.seatsSelected = { num: value };\n        this.showBus = false;\n      } else if (isSelected && isFree) {\n        this.seatsSelected = {};\n      }\n\n      this.$validator.validate(`passenger__place_${this.raceId}`, this.seatsSelected);\n      this.$emit('selectedPlace', this.seatsSelected, this.raceId);\n    },\n    setFloor(floor) {\n      this.floor = floor;\n    },\n  },\n  computed: {\n    route() {\n      return `${this.departure} - ${this.arrived}`;\n    },\n    busIcon() {\n      return faBus;\n    },\n    selectedSeat() {\n      const num = this.seatsSelected.num;\n\n      if (num) {\n        return `${num} `;\n      }\n\n      return '';\n    },\n    getPlace() {\n      return this.seatsSelected.num ? this.seatsSelected.num : '';\n    },\n    getButtonLabel() {\n      return this.seatsSelected.num ? this.$t('bus.change') : this.$t('bus.choise');\n    },\n    seats() {\n      const rows = [];\n      let cSeat = 1;\n\n      while (this.busCapacity >= cSeat) {\n        const row = [];\n\n        for (let i = 0; i < 4; i += 1) {\n          const secondSide = i === 2;\n          const seat = new Seat(cSeat, this.raceId);\n          const isFree = Boolean(filter(this.places, { num: seat.number }).length);\n          const isSelected = this.seatsSelected.num === cSeat;\n          const isEmpty = !(this.busCapacity >= cSeat);\n\n          seat.secondSide = secondSide;\n          seat.isSelected = isSelected;\n          seat.isFree = isFree;\n          seat.isEmpty = isEmpty;\n\n          row.push(seat);\n          cSeat += 1;\n        }\n\n        rows.push(row);\n      }\n\n      return rows.reverse();\n    },\n    seatsTempl() {\n      const rows = [];\n      const templFloor = this.busTemplate.matrix[this.floor - 1];\n      const { x, y } = this.busTemplate.size[this.floor - 1];\n\n      for (let i = 0; i < x; i += 1) {\n        const row = [];\n\n        for (let k = 0; k < y; k += 1) {\n          const templSeat = templFloor[k][i];\n          const seat = new Seat(templSeat.n, this.raceId);\n          const isFree = Boolean(filter(this.places, { num: seat.number }).length);\n          const isSelected = (this.seatsSelected.num === templSeat.n) && seat.number;\n          const isEmpty = !seat.number;\n\n          seat.isSelected = isSelected;\n          seat.isFree = isFree;\n          seat.isEmpty = isEmpty;\n\n          row.push(seat);\n        }\n\n        rows.push(row.reverse());\n      }\n\n      return rows.reverse();\n    },\n  },\n  filters: {\n  },\n  created() {\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n#app, #auth {\n  .octobus__dropdown-wrapper.is-active {\n    position: fixed;\n    left: 0;\n    right: 0;\n    top: 0;\n    bottom: 0;\n    z-index: 8;\n  }\n\n  .octobus__dropdown {\n    // z-index: 9;\n    width: 100%;\n\n    .octobus__dropdown-trigger {\n      width: 100%;\n    }\n  }\n\n  .bus {\n    display: inline-flex;\n    margin-top: 10px;\n    padding: 20px 15px 20px 25px;\n    border-radius: 8px;\n    border: 1px solid #ccc;\n    padding: 20px;\n\n    .seat {\n      width: 40px;\n      max-width: 40px;\n      height: 40px;\n      max-height: 40px;\n      margin: 2px;\n      color: #ccc;\n      fill: #ccc;\n      overflow: hidden;\n      position: relative;\n\n      .seat--chair{\n        max-height: 40px;\n        line-height: 40px;\n      }\n\n      .seat--number {\n        color: #fff;\n        font-size: 12px;\n        font-weight: 700;\n        position: absolute;\n        left: 58%;\n        top: 50%;\n        transform: translate(-50%, -50%);\n      }\n\n      &.seat-free {\n        cursor: pointer;\n      }\n\n      &.seat-disabled {\n        cursor: not-allowed;\n      }\n\n      &.empty {\n        border: none;\