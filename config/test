                   :init-count=\"adult\"\n                          :min=\"1\"\n                          @counterChange=\"setPassengers\"\n                          age=\"adult\"\n                          :sum=\"passengers\"\n                          :can-add-more=\"validateCount\"\n                        />\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n              <div class=\"octobus__column is-three-quarters-mobile\">\n                <div class=\"octobus__control\">\n                  <button\n                    class=\"octobus__button is-primary is-fullwidth is-medium\"\n                    :disabled=\"!validate\"\n                    @click=\"onClick\"\n                    :class=\"{'is-loading': searching}\"\n                  >{{$t('search.search')}}</button>\n                </div>\n              </div>\n            </div>\n          </div>\n      </div>\n      <!-- For touch -->\n      <div class=\"octobus__columns is-hidden-desktop\">\n        <div class=\"octobus__column\">\n          <Currency @setCurrency=\"setCurrency\" :currencyList=\"currencyList\" customClass=\"has-text-right\" v-show=\"$searchCurrency\" />\n        </div>\n      </div>\n      <div class=\"octobus__columns is-vcentered\">\n        <div class=\"octobus__column is-one-quarter-desktop is-4-tablet is-hidden-touch\">\n          <div class=\"field\" v-show=\"showAdditionalSearch\">\n            <input\n              v-model=\"directPathChecked\"\n              id=\"directPath\"\n              type=\"checkbox\"\n              name=\"directPath\"\n              class=\"switch is-thin\"\n            >\n            <label for=\"directPath\">{{$t('search.direct')}}</label>\n          </div>\n        </div>\n        <div class=\"octobus__column is-hidden-touch\">\n          <div class=\"octobus__field\" v-show=\"showAdditionalSearch\">\n            <input\n              v-model=\"bothSideChecked\"\n              id=\"bothSide\"\n              type=\"checkbox\"\n              name=\"bothSide\"\n              class=\"switch is-thin\"\n            >\n            <label for=\"bothSide\">{{$t('search.bothside')}}</label>\n          </div>\n        </div>\n        <div class=\"octobus__column is-hidden-touch octobus__datepicker-backdate\">\n          <Datepicker\n            id=\"backdate\"\n            name=\"backdate\"\n            :placeholder=\"$t('search.backdate')\"\n            :input-class=\"getClass('backdate')\"\n            :disabled=\"{to: yesterday}\"\n            :language=\"componentLocale\"\n            :monday-first=\"true\"\n            :required=\"true\"\n            @selected=\"setDatepicker($event, 'backdate')\"\n            v-model=\"backdate\"\n            calendar-class=\"datepicker-when\"\n            v-if=\"bothSide\"\n            :calendar-button=\"true\"\n          />\n        </div>\n        <div class=\"octobus__column is-hidden-touch\">\n          <Currency @setCurrency=\"setCurrency\" :currencyList=\"currencyList\" v-show=\"$searchCurrency\" />\n        </div>\n      </div>\n      <!-- For touch -->\n      <div class=\"octobus__columns is-hidden-desktop\">\n        <div class=\"octobus__column\">\n          <button\n            class=\"octobus__button is-primary is-fullwidth is-medium\"\n            :disabled=\"!validate\"\n            @click=\"onClick\"\n            :class=\"{'is-loading': searching}\"\n          >{{$t('search.search')}}</button>\n        </div>\n      </div>\n    </div>\n    <NewPassword\n      v-if=\"resetModal\"\n      :show=\"resetModal\"\n      :token=\"token\"\n      @hideModal=\"onHideModal\"\n    />\n  </section>\n</template>\n\n<script>\nimport api from '@/api';\nimport moment from 'moment';\nimport Datepicker from 'vuejs-datepicker';\nimport FontAwesomeIcon from '@fortawesome/vue-fontawesome';\nimport { faUser, faRetweet } from '@fortawesome/fontawesome-free-solid';\nimport Autosuggest from '@/components/Autosuggest/Autosuggest';\nimport Counter from '@/components/Counter/Counter';\nimport Currency from '@/components/_common/Currency';\nimport NewPassword from '@/components/_common/NewPassword';\nimport { isUrlLang, getComponentLocale } from '@/utils/utils';\n\nexport default {\n  name: 'Search',\n  components: { Autosuggest, Datepicker, Counter, Currency, FontAwesomeIcon, NewPassword },\n  props: {\n    addinionalClass: {\n      type: String,\n      default() {\n        return '';\n      },\n    },\n    async: {\n      type: Boolean,\n      default: true,\n    },\n    initialCities: {\n      type: Array,\n      default() {\n        return [];\n      },\n    },\n    initialFrom: {\n      type: Object,\n      default() {\n        return null;\n      },\n    },\n    initialTo: {\n      type: Object,\n      default() {\n        return null;\n      },\n    },\n    initialWhen: {\n      default() {\n        return null;\n      },\n    },\n    initialAdult: {\n      type: Number,\n      default() {\n        return 1;\n      },\n    },\n    initialChildren: {\n      type: Number,\n      default() {\n        return 0;\n      },\n    },\n    initialBackDate: {\n      default() {\n        return null;\n      },\n    },\n    initialIsBack: {\n      type: Number,\n      default() {\n        return 0;\n      },\n    },\n    initialChange: {\n      type: [Number, Boolean],\n      default() {\n        return 0;\n      },\n    },\n  },\n  data() {\n    return {\n      cities: this.initialCities,\n      from: this.initialFrom,\n      to: this.initialTo,\n      when: this.initialWhen,\n      lang: this.$framelocale,\n      yesterday: new Date(moment().subtract(1, 'd')),\n      adult: this.initialAdult,\n      children: this.initialChildren,\n      backdate: this.initialBackDate,\n      bothSide: this.$searchBack ? 1 : this.initialIsBack,\n      directPath: this.$searchChanges ? 1 : this.initialChange,\n      currency: 'UAH',\n      currencyList: this.$currencyList,\n      searching: false,\n      resetModal: false,\n      token: '',\n      componentLocale: getComponentLocale(this.$framelocale),\n    };\n  },\n  /* eslint-disable object-shorthand */\n  watch: {\n    '$route.fullPath'() {\n      this.when = this.$route.query.when;\n    },\n    'initialBackDate'(newValue) {\n      this.backdate = newValue;\n    },\n  },\n  methods: {\n    onHideModal() {\n      this.resetModal = false;\n\n      // eslint-disable-next-line no-unused-vars\n      const { token: _, queryWithoutToken } = this.$route.query;\n      this.$router.replace({ query: queryWithoutToken });\n    },\n    setAutosuggest(data) {\n      const [type, item] = data;\n      this[type] = item;\n\n      if (type === 'to') {\n        const el = document.querySelector('.octobus__datepicker-when .vdp-datepicker__calendar-button');\n\n        if (el) {\n          el.click();\n        }\n      }\n    },\n    switchRoutes() {\n      const { from, to } = this;\n\n      this.$set(this, 'from', to);\n      this.$set(this, 'to', from);\n    },\n    setDatepicker(newDate, datepicker) {\n      this[datepicker] = newDate;\n      if (datepicker === 'when' && this.bothSide === true) {\n        const el = document.querySelector('.octobus__datepicker-backdate .vdp-datepicker__calendar-button');\n\n        if (el) {\n          el.click();\n        }\n      }\n    },\n    // getCity(id) {\n    //   return this.cities.filter(city => Number(city.id) === Number(id))[0];\n    // },\n    onClick() {\n      if (this.validate) {\n        this.searching = true;\n        this.redirect();\n        this.searching = false;\n        return true;\n      }\n\n      return false;\n    },\n    setCurrency(value) {\n      this.currency = value;\n    },\n    getClass() {\n      const primary = 'octobus__input is-medium';\n\n      return `${primary} is-primary`;\n    },\n    isBackDateValid() {\n      return this.backdate !== null && moment(this.backdate).isSameOrAfter(this.when);\n    },\n    redirect() {\n      const when = moment(this.when).format('YYYY-MM-DD');\n      let searchParams = `?from=${this.from.id}&to=${this.to.id}&when=${when}&adult=${this.adult}&children=${this.children}&currency=${this.currency}`;\n      let backdate = null;\n      const lang = isUrlLang ? `/${this.$UrlLang}` : '';\n\n      if (this.directPath) {\n        searchParams += '&change=1';\n      } else {\n        searchParams += '&change=0';\n      }\n\n      if (this.bothSide && this.backdate) {\n        backdate = moment(this.backdate).format('YYYY-MM-DD');\n        searchParams += `&goback=1&whenback=${backdate}`;\n      }\n\n      // for viabona\n      if (this.$search) {\n        const data = {\n          from: this.from.id,\n          to: this.to.id,\n          when: when,\n          adult: this.adult,\n          children: this.children,\n          currency: this.currency,\n          change: Number(this.directPath),\n          ...(this.bothSide && this.backdate) && { backdate: `&goback=1&whenback=${backdate}` },\n        };\n\n        this.$emit('search:new', data);\n      } else {\n        const { children } = this.$router.options.routes[1];\n        const route = children.find(r => r.name === 'Result');\n        /* eslint-disable */\n        if (this.$framespa !== true && (this.$route.name === 'Main' || this.$route.name === 'MainWithRedirect')) {\n          if (this.$newTab) {\n            window.open(`${lang}/${route.path}${searchParams}`);\n          } else {\n            location.href = `${lang}/${route.path}${searchParams}`;\n          }\n        } else if (this.$newTab) {\n          let routeData = this.$router.resolve(`${lang}/${route.path}${searchParams}`);\n          window.open(routeData.href, '_blank');\n        } else {\n          this.$router.push(`${lang}/${route.path}${searchParams}`);\n        }\n      }\n\n      localStorage.setItem('__octobus__search-cnt', this.passengers);\n    },\n    setPassengers(value, type) {\n      this[type] = value;\n    },\n    setAditionalParam(id, status) {\n      if (id === 'bothSide' && this.directPath && status) {\n        this.directPath = !this.directPath;\n      }\n\n      if (id === 'directPath' && this.bothSide && status) {\n        this.bothSide = !this.bothSide;\n      }\n\n      this[id] = status;\n    },\n  },\n  computed: {\n    validate() {\n      if (\n        this.when !== null &&\n        !(this.bothSide && !this.isBackDateValid()) &&\n        (this.from || {}).id &&\n        (this.to || {}).id &&\n        !!this.currency\n      ) {\n        return true;\n      }\n\n      return false;\n    },\n    retweetIcon() {\n      return faRetweet;\n    },\n    showAdditionalSearch() {\n      return !this.$hideAdditionalSearch;\n    },\n    passengers() {\n      return Number(this.adult) + Number(this.children) || 1;\n    },\n    validateCount() {\n      return this.passengers < 10;\n    },\n    iconUser() {\n      return faUser;\n    },\n    bothSideChecked: {\n      get() {\n        return this.bothSide;\n      },\n      set(newVal) {\n        this.setAditionalParam('bothSide', newVal);\n      },\n    },\n    directPathChecked: {\n      get() {\n        return this.directPath;\n      },\n      set(newVal) {\n        this.setAditionalParam('directPath', newVal);\n      },\n    },\n    isFullWidth() {\n      return this.$fullwidth;\n    },\n  },\n  created() {\n    if (this.async) {\n      api.Content.cities().then((res) => {\n        this.cities = res;\n\n        // if (this.$search) {\n        //   this.from = this.getCity(this.$search.from);\n        //   this.to = this.getCity(this.$search.to);\n        // }\n      });\n    }\n  },\n  mounted() {\n    localStorage.setItem('__octobus__search-cnt', this.passengers);\n\n    const { token } = this.$route.query;\n\n    if (token) {\n      this.resetModal = true;\n      this.token = token;\n    }\n  },\n};\n</script>\n\n<style scope lang=\"scss\">\n/* Large Devices, Wide Screens */\n@media only screen and (min-width : 768px) {\n  #app, #auth {\n    .search {\n      s