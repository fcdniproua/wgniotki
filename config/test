  paysys: {\n      type: String,\n    },\n  },\n  data() {\n    return {};\n  },\n  computed: {\n    emailIcon() {\n      return faEnvelope;\n    },\n    mask() {\n      const phone = this.phone.country;\n      const mask = getMask(phone);\n\n      return mask;\n    },\n    isPaysys() {\n      return this.$frameparams.paysys.length > 1;\n    },\n    isLiqpay() {\n      return this.$frameparams.paysys.includes('liqpay');\n    },\n    isFondy() {\n      return this.$frameparams.paysys.includes('fondy');\n    },\n    isWayforpay() {\n      return this.$frameparams.paysys.includes('wayforpay');\n    },\n    isYandex() {\n      return this.$frameparams.paysys.includes('yandex');\n    },\n    isPayU() {\n      return this.$frameparams.paysys.includes('payu');\n    },\n    isAdyen() {\n      return this.$frameparams.paysys.includes('adyen');\n    },\n    isAuth() {\n      return this.$frameauth;\n    },\n  },\n  methods: {\n    updatePhone(value, type) {\n      if (type === 'number') {\n        const formattedValue = value.trim();\n\n        this.$emit('changePhone', String(formattedValue), type);\n        return true;\n      }\n\n      this.$emit('changePhone', String(value), type);\n      return true;\n    },\n    updatePaysys(paysys) {\n      this.$emit('paysys:changed', paysys);\n    },\n    updateEmail(value) {\n      const formattedValue = value.trim();\n\n      this.$emit('changeEmail', String(formattedValue));\n    },\n    isError(field) {\n      return this.errors.has(field);\n    },\n    isSuccess(field) {\n      return (\n        !this.errors.has(field) &&\n        this.fields[field] &&\n        this.fields[field].dirty\n      );\n    },\n    getClassValidate(field) {\n      if (this.isError(field)) {\n        return 'has-text-danger';\n      }\n\n      if (this.isSuccess(field)) {\n        return 'has-text-primary';\n      }\n\n      return '';\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n#app, #auth {\n  select {\n    &.is-danger {\n      bord